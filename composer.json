{
    "name": "gisostallenberg/pdo-clause-helper",
    "description": "Utility to create placeholder strings to use in WHERE ... IN () clauses",
    "type": "library",
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Giso Stallenberg",
            "email": "gisostallenberg@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GisoStallenberg\\PDOClauseHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GisoStallenberg\\PDOClauseHelper\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    }
}
